{
	"name": "electron-gradle-example",
	"version": "1.1.1",
	"description": "Application to prove the viability of dynamically creating and running Gradle build scripts using Electron, Javascript, and Node.js",
	"main": "main.js",
	"scripts": {
		"start": "./node_modules/.bin/electron-rebuild && ./node_modules/.bin/node-java-maven && electron main.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/linfirmware/electron-gradle-example.git"
	},
	"keywords": [
		"Electron",
		"Gradle",
		"build",
		"tooling",
		"project"
	],
	"author": "Lin Firmware, Wesley Carlsen, Red Pill Analytics",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/linfirmware/electron-gradle-example/issues"
	},
	"homepage": "https://github.com/linfirmware/electron-gradle-example#readme",
	"devDependencies": {
		"electron-prebuilt": "^0.36.11",
		"electron-rebuild": "^1.1.3",
		"java": "^0.6.1",
		"node-java-maven": "^0.0.12"
	},
	"java": {
		"repositories": [
			{
				"url": "http://repo.gradle.org/gradle/libs-releases-local/"
			}
		],
		"dependencies": [
			{
				"groupId": "org.gradle",
				"artifactId": "gradle-tooling-api",
				"version": "2.6"
			}
		]
	}
}
